{
  "name": "web-fe-lint",
  "version": "0.0.19",
  "description": "前端编码规范工程化标准脚手架",
  "type": "module",
  "bin": {
    "web-fe-lint": "lib/cli.js"
  },
  "main": "./lib/index.js",
  "scripts": {
    "dev": "pnpm run copyfiles && tsc -w",
    "build": "rm -rf lib && pnpm run copyfiles && tsc",
    "copyfiles": "copyfiles -a -u 1 \"src/config/**\" lib",
    "test": "pnpm run build && jest",
    "coverage": "nyc jest --silent --forceExit",
    "prepublishOnly": "pnpm run test"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "encode",
    "lint",
    "cli"
  ],
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(execa)/)"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    }
  },
  "devDependencies": {
    "@types/command-exists": "^1.2.3",
    "@types/cross-spawn": "^6.0.6",
    "@types/ejs": "^3.1.5",
    "@types/eslint": "^9.6.1",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^9.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.3.0",
    "@types/prettier": "^3.0.0",
    "@types/stylelint": "^14.0.0",
    "@types/text-table": "^0.2.5",
    "copyfiles": "^2.4.1",
    "jest": "^30.1.0",
    "nyc": "^17.1.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@babel/core": "^7.28.3",
    "@babel/eslint-parser": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@commitlint/cli": "^19.8.1",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "chalk": "^5.6.0",
    "command-exists": "^1.2.9",
    "commander": "^14.0.0",
    "cross-spawn": "^7.0.6",
    "ejs": "^3.1.10",
    "eslint": "^9.34.0",
    "eslint-config-egg": "^14.1.0",
    "eslint-config-encode": "^1.0.8",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-vue": "^10.4.0",
    "execa": "6.1.0",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.1",
    "glob": "^11.0.3",
    "husky": "^9.1.7",
    "inquirer": "^12.9.4",
    "is-docker": "^3.0.0",
    "lodash": "^4.17.21",
    "markdownlint": "^0.38.0",
    "markdownlint-rule-helpers": "^0.29.0",
    "ora": "^8.2.0",
    "prettier": "^3.6.2",
    "strip-ansi": "^7.1.0",
    "stylelint": "^16.23.1",
    "stylelint-scss": "^6.12.1",
    "terminal-link": "^4.0.0",
    "text-table": "^0.2.0",
    "vue-eslint-parser": "^10.2.0",
    "web-commitlint-config": "^0.0.10",
    "web-markdownlint-config": "^0.0.10"
  },
  "author": "miaomiao.zhou <miaomiao.zhou@qq.com>",
