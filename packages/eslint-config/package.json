{
  "name": "web-eslint-config",
  "version": "0.0.6",
  "description": "JavaScript TypeScript Node Vue React 语法规范lint检查",
  "keywords": [
    "javascript",
    "typescript",
    "vue",
    "react",
    "node",
    "lint"
  ],
  "author": "miaomiao.zhou <miaomiao.zhou@qq.com>",
  "homepage": "https://github.com/zmm520study/fe-spec#reademe",
  "license": "ISC",
  "main": "eslint.config.mjs",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zmm520study/fe-spec.git"
  },
  "scripts": {
    "lint": "eslint ./ --config eslint.config.mjs",
    "lint:fix": "eslint ./ --config eslint.config.mjs  --fix",
    "test": "mocha ./__tests__/*.test.js --timeout 5000",
    "print-config": "eslint --print-config ./eslint.config.mjs > ./print-config.json"
  },
  "bugs": {
    "url": "https://github.com/zmm520study/fe-spec/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/eslint-parser": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@eslint/js": "^9.34.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.34.0",
    "eslint-config-egg": "^14.1.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-jsx-plus": "^0.1.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^16.3.0",
    "lodash": "^4.17.21",
    "mocha": "^11.7.1",
    "typescript": "^5.9.2",
    "vue-eslint-parser": "^10.2.0"
  },
